{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "universal-router": "^3.2.0"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "react-scripts": "1.0.10",
    "stylelint": "^8.0.0",
    "stylelint-config-primer": "^2.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.6.0"
  },
  "scripts": {
    "start": "node tools/setup && react-scripts start",
    "build": "node tools/setup && react-scripts build",
    "test": "node tools/setup && react-scripts test --env=jsdom",
    "lint": "eslint src tools && stylelint \"src/**/*.css\"",
    "lint-fix": "eslint src tools --fix & stylelint \"src/**/*.css\" --fix",
    "deploy": "node tools/setup && react-scripts build && gh-pages -d build"
  }
